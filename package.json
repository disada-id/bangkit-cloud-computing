{
  "name": "bangkit-cloud-computing",
  "version": "1.0.0",
  "description": "Bangkit Academy 2023 Batch Capstone Project",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "start-dev": "nodemon src/index.js",
    "migrate-dev-generate": "cross-env NODE_ENV=development npx sequelize-cli migration:generate --name",
    "migrate-dev": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "migrate-dev-undo-all": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo:all",
    "migrate-dev-undo": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo --name",
    "migrate-dev-status": "cross-env  NODE_ENV=development npx sequelize-cli db:migrate:status",
    "migrate-dev-debug": "cross-env NODE_ENV=development npx sequelize-cli db:migrate --debug",
    "migrate-prod": "cross-env NODE_ENV=production npx sequelize-cli db:migrate",
    "seed-dev": "cross-env NODE_ENV=development npx sequelize-cli db:seed:all",
    "seed-prod": "cross-env NODE_ENV=production npx sequelize-cli db:seed:all",
    "lint": "eslint src/"
  },
  "author": "CH2-PS091",
  "license": "ISC",
  "dependencies": {
    "@hapi/hapi": "^21.3.2",
    "@hapi/inert": "^7.1.0",
    "@hapi/joi": "^17.1.1",
    "@hapi/jwt": "^3.2.0",
    "@tensorflow/tfjs": "^4.14.0",
    "@tensorflow/tfjs-node": "^2.8.5",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "hapi-swagger": "^17.2.0",
    "joi": "^17.11.0",
    "joi-password": "^3.0.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "nanoid": "^3.3.7",
    "ndarray": "^1.0.19",
    "ndarray-scratch": "^1.2.0",
    "node-mfcc": "^0.0.2",
    "node-wav": "^0.0.2",
    "noise-gate": "^0.0.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "sequelize-cli": "^6.6.2"
  }
