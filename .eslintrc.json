{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "import/no-unresolved": "off",
    "no-unused-vars": "off",
    "linebreak-style": "off",
    "lines-around-directive": "off",
    "strict": "off",
    "global-require": "off",
    "import/no-dynamic-require": "off"
  }
}
